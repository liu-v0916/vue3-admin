{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "vite/**/*.ts",
    "vite.config.ts",
    "vite/**/*.ts",
    "mock/**/*.ts",
    "types/**/*.d.ts",
    "types/**/*.ts"
  ]
}
